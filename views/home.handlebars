<div class='container'>
    {{> logo}}
    <div class="row">
        <div class='heading'>
            <h1 class="teamName">Seapoint Moms</h1></div>
    </div>
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">{{> navigation}}</div>
        <div class="col-md-3"></div>
    </div>
    <div class="row">
        <div class="col-md-3"></div>
        <div class="col-md-6">
            <div class='feed'>
                <ul>
                    <li class="homeActivity">
                        <span class="profilePic">
                          <img src='/images/Molly.jpg'>
                        </span>

                        <span class="text">
                          Molly has created the Seapoint Moms team.
                        </span>
                    </li>
                    <li class="homeActivity">
                        <span class="profilePic">
                          <img src ='/images/Kate.jpg'>
                        </span>
                        <span class="text">
                          Kate has joined the team.
                        </span>
                    </li>

                    {{#goals}}
                        <li class="homeActivity">
                            <span class="profilePic">
                              <img src='/images/{{user_name}}.jpg'>
                            </span>

                            <span class="text">
                              {{user_name}} wants to {{goal}}
                            </span>
                        </li>
                    {{/goals}}
                </ul>
            </div>
        </div>
        <div class="col-md-3"></div>
    </div>
</div>
